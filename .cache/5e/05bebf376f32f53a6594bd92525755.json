nal":{"line":3,"column":29}},{"name":"Carousel","generated":{"line":109,"column":15},"source":"Carousel.js","original":{"line":52,"column":15}}],"sources":{"Carousel.js":"import React from \"react\";\n\nclass Carousel extends React.Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      photos: [],\n      active: 0\n    };\n    this.handleClickIndex = this.handleClickIndex.bind(this);\n  }\n  static getDerivedStateFromProps({ media }) {\n    let photos = [];\n\n    if (media && media.photos && media.photos.photo) {\n      photos = media.photos.photo.filter(photo => photo[\"@size\"] === \"pn\");\n    }\n\n    return { photos };\n  }\n  //React synthetic event\n  handleClickIndex(event) {\n    this.setState({\n      active: +event.target.dataset.index\n    });\n  }\n\n  render() {\n    const { photos, active } = this.state;\n    return (\n      <div className=\"carousel\">\n        <img src={photos[active].value} alt=\"primary animal\" />\n        <div className=\"carousel-smaller\">\n          {photos.map((photo, index) => (\n            /*eslint-disable-next-line */\n            <img\n              onClick={this.handleClickIndex}\n              key={photo.value}\n              //set index to data attribute\n              data-index={index}\n              src={photo.value}\n              //ternary opetator to set active state\n              className={index === active ? \"active\" : \"\"}\n              alt=\"animal thumbnail\"\n            />\n          ))}\n        </div>\n      </div>\n    );\n  }\n}\nexport default Carousel;\n"},"lineCount":null}},"hash":"3b25088140dc43e0370f793a781484e4","cacheData":{"env":{}}}